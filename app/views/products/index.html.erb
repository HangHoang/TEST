<div class="page-header">
	<h1>List products of <%= @product_type.product_type_name%></h1>
</div>
<div>
  <%= link_to 'New product', new_product_type_product_path(@product_type), :class => "btn btn-mini" %>
  <%= link_to 'Back', product_types_path, :class => 'btn btn-mini'%>
</div>

<div class="table-content">
  <%= render "products" %>
</div>
<div class =" col-lg-4 col-lg-offset-5 col-md-4 col-md-offset-5 col-xs-* my_pagination">
  <%=will_paginate @products%>
</div>


<div class="dialog" title ="Confirm Delete">
  <p>Are U sure?</p>
</div>
<script type="text/javascript">
$(document).ready(function(){
  $(".dialog").hide();
});
</script>